<% layout("/layout/boilerplate.ejs") %>

    <h3>all listen</h3>
    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
        <% for(let listens of alllisten){%>
            <a href="/listening/<%=listens.id%>" class="listing-link">
    <div class="card listening-card col" >
  <img src="<%= listens.image.url%>" class="card-img-top" alt="listening-image" style="height: 20rem;">
 
  <div class="card-body">
    <p class="card-text">
       <b> <%= listens.title%>  </b><br />
        &#8377;<%= listens.price.toLocaleString('en-IN') %>/night 
    </p>
  </div>
</div>
</a>
<% } %>
    </div>
 
    
